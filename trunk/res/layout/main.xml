<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_height="fill_parent"
	android:layout_width="fill_parent">

	<TextView android:id="@+id/TextView02" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="Swap Control"
		android:textSize="4mm"></TextView>
	<LinearLayout android:id="@+id/SwapLayout"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<Button android:id="@+id/SwapOn_32" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Swap ON (32 MB)"></Button>
		<Button android:id="@+id/SwapOff" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Swap OFF"></Button>


	</LinearLayout>
	<TextView android:id="@+id/TextView01" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textSize="4mm"
		android:text="Swappiness"></TextView>
	<LinearLayout android:id="@+id/SwappinessLayout"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<Button android:id="@+id/GetSwappiness" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Get"></Button>
		<EditText android:layout_height="wrap_content" android:text="0"
			android:singleLine="true" android:saveEnabled="true"
			android:maxLength="3" android:layout_width="wrap_content"
			android:ellipsize="none" android:id="@+id/Swappiness"></EditText>
		<Button android:id="@+id/SetSwappiness" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Set"></Button>



		<Button android:id="@+id/SetSwappiness10" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Set 10"></Button>
	

</LinearLayout><LinearLayout android:id="@+id/LinearLayout01" android:layout_width="wrap_content" android:layout_height="wrap_content"><Button android:id="@+id/GetInfo" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Get information"></Button><Button android:text="Settings" android:id="@+id/StartSettings" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
</LinearLayout><ScrollView android:id="@+id/ScrollView01" android:layout_width="wrap_content" android:layout_height="wrap_content"><TextView android:id="@+id/LogText" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView></ScrollView>
	
	









</LinearLayout>
