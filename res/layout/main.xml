<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_height="fill_parent"
	android:layout_width="fill_parent">
	<ScrollView android:id="@+id/ScrollView02"
		android:layout_width="fill_parent" android:layout_gravity="center"
		android:layout_height="260px">
		<TableLayout android:id="@+id/TableLayout01"
			android:layout_gravity="center" android:layout_height="fill_parent"
			android:layout_width="wrap_content">
			<TextView android:id="@+id/TextView01" android:layout_height="wrap_content" android:text="Swap control" android:layout_width="fill_parent" android:textSize="22px"></TextView>
			<Button android:id="@+id/SwapOn_32" android:layout_height="fill_parent" android:layout_width="fill_parent" android:text="Swap ON"></Button><Button android:id="@+id/SwapOff" android:text="Swap OFF" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
			
			
			<TextView android:id="@+id/TextView01" android:layout_height="wrap_content" android:layout_width="fill_parent" android:textSize="22px" android:text="Configuration"></TextView>
			<Button android:text="Settings" android:id="@+id/StartSettings" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button><Button android:id="@+id/GetInfo" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Get information"></Button>
		</TableLayout>
	</ScrollView>
	<ScrollView android:id="@+id/ScrollView01"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:id="@+id/LogText" android:layout_width="wrap_content"
			android:layout_height="wrap_content"></TextView>
	</ScrollView>
</LinearLayout>
